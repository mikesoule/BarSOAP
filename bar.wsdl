    <wsdl:message name="GetMenuRequest"/>
    <wsdl:message name="GetMenuResponse">
        <wsdl:part name="MenuRS" element="MenuRS"/>
    </wsdl:message>
    <wsdl:message name="GetDrinkRequest">
        <wsdl:part name="GetDrinkRQ" element="GetDrinkRQ"/>
    </wsdl:message>
    <wsdl:message name="GetDrinkResponse">
        <wsdl:part name="GetDrinkRS" element="GetDrinkRS"/>
    </wsdl:message>
    <wsdl:portType name="BarPortType">
        <wsdl:operation name="GetMenu">
            <wsdl:input name="GetMenuRequest"/>
            <wsdl:output name="GetMenuResponse" message="GetMenuRS"/>
        </wsdl:operation>
        <wsdl:operation name="GetDrink">
            <wsdl:input name="GetDrinkRequest" message="GetDrinkRQ"/>
            <wsdl:output name="GetDrinkResponse" message="GetDrinkRQ"/>
        </wsdl:operation>
    </wsdl:portType>
